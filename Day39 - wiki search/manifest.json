{
    "manifest_version": 2,
    "name": "wikipedia searcher",
    "version": "0.1",
    "description": "search wikipedia on go",
    "icons": {
        "128": "icon.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "contextMenus"
    ]
}