{
    "manifest_version": 2,
    "name": "Coding Train Extension 2",
    "version": "0.001",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "background" : {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "ct.png"
    }
}